<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TranslateActivity">

    <TextView
        android:id="@+id/txtketqua"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tutienganh"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.149"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtketqua"
        app:layout_constraintTop_toTopOf="@id/txtketqua"
        app:layout_constraintBottom_toBottomOf="@id/txtketqua"
        app:srcCompat="@drawable/ic_volume_up"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/txtvie"
        app:layout_constraintTop_toTopOf="@id/txtvie"
        app:layout_constraintBottom_toBottomOf="@id/txtvie"
        app:srcCompat="@drawable/arrow_right"/>

    <TextView
        android:id="@+id/txtvie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tutiengviet"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@id/txtketqua"
        app:layout_constraintTop_toBottomOf="@id/txtketqua"/>



</androidx.constraintlayout.widget.ConstraintLayout>